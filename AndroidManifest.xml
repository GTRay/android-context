<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="edu.fsu.cs.contextprovider">
	
	<application android:label="@string/app_name" android:icon="@drawable/cw">
		<provider android:name="ContextProvider"
			android:authorities="edu.fsu.cs.contextprovider" />
			
		<activity android:name="ContextBrowserActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		
	</application>
	
	<supports-screens android:largeScreens="true"
		android:normalScreens="true" android:smallScreens="true"
		android:anyDensity="true" />
		
</manifest>